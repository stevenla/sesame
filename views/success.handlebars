<?xml version="1.0" encoding="UTF-8"?>
<Response>
    <Say voice="{{ config.voice }}" language="{{ config.language }}">{{ config.successMessage }}</Say>
	{{#each config.successTone}}
	    <Play>/static/{{this}}.wav</Play>
	{{/each}}
	{{#if notify}}
	<Sms to="{{notify}}">SESAME: your guest has arrived</Sms>
	{{/if}}
</Response>
